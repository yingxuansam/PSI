platform :ios, '9.0'

target 'PSI' do

  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  inherit! :search_paths

  # Pods for PSI

    pod 'SwiftyJSON'
    pod 'Alamofire'
    pod 'SVProgressHUD'
  
  abstract_target 'Tests' do
   target "PSITests"

    pod 'Quick'
    pod 'Nimble'
  end

end


